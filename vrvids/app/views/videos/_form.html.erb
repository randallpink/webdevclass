<%= simple_form_for(@video) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :link_url %>
     <%= f.input :slug %>
    <%= current_user.email %>
    <%= f.association :category %>
    <%= f.input :description %>
    <%= f.input :city %>
    <%= f.input :state %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
